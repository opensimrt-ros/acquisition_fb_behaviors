<?xml version="1.0" encoding="UTF-8"?>

<behavior name="insole_upright">

	<executable package_path="acquisition_fb_flexbe_behaviors.insole_upright_sm" class="insole_uprightSM" />
	<tagstring>acquisition, tmux, sound, old_heading, insoles</tagstring>
	<author>frekle</author>
	<date>Mon Apr 22 2024</date>
	<description>
		acquire with tmux,
old heading, 
playing sounds,
with insoles and ID
staring upright
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="text" name="trail_name" default="walking" label="trail_name" hint="Sets the trail_name" />

		<param type="text" name="subject_id" default="" label="subject_id" hint="Sets the subject_id" />

		<param type="text" name="trial_save_path" default="" label="trial_save_path" hint="Sets the trial_save_path" />

		<param type="numeric" name="subject_height" default="0" label="subject_height" hint="Sets the subject_height [m]">
			<min value="0" />
			<max value="3" />
		</param>

		<param type="numeric" name="subject_age" default="0" label="subject_age" hint="Sets the subject_age [years]">
			<min value="0" />
			<max value="150" />
		</param>

		<param type="numeric" name="subject_weight" default="0" label="subject_weight" hint="Sets the subject_weight [kg]">
			<min value="0" />
			<max value="200" />
		</param>

		<param type="enum" name="subject_shoe_size" default="S6 (42-43)" label="subject_shoe_size" hint="Sets the subject_shoe_size">
			<option value="S3 (36-37)" />
			<option value="S4 (38-39)" />
			<option value="S6 (42-43)" />
		</param>

	</params>


</behavior>