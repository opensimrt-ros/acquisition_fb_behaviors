<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Acquire_everything">

	<executable package_path="acquisition_fb_flexbe_behaviors.acquire_everything_sm" class="Acquire_everythingSM" />
	<tagstring>acquisition, tmux, sound, old_heading, upright, ar</tagstring>
	<author>frekle</author>
	<date>Mon Apr 22 2024</date>
	<description>
		acquire using embeddable IMU behavior
- with tmux 
- IMUs with upright start
- IK using old heading 
- playing sounds
- now tries to add camera as well
	</description>


